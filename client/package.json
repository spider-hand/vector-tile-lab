{
  "name": "client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint:style": "stylelint \"src/**/*.vue\" --fix",
    "format": "prettier --write src/",
    "prepare": "cd .. && husky",
    "generate:api": "openapi-generator generate -i ../server/schema.yaml -g typescript-fetch -o ./src/services"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/vue-query": "^5.89.0",
    "@tanstack/vue-table": "^8.21.3",
    "@vueuse/core": "^13.9.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "install": "^0.13.0",
    "lucide-vue-next": "^0.544.0",
    "maplibre-gl": "^5.7.1",
    "npm": "^11.6.1",
    "pmtiles": "^4.3.0",
    "reka-ui": "^2.5.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.8"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "stylelint": "^16.24.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "src/**/*.vue": [
      "stylelint --fix"
    ]
  }
}
